source ~/.vimrc

nnoremap <leader>e   :action FindModal<cr>
nnoremap <leader>m   :action Annotate<cr>
nnoremap <leader>n   :action SearchEverywhere<cr>
nnoremap <leader>gd  :action GotoDeclaration<cr>
nnoremap <leader>gi  :action GotoImplementation<cr>
nnoremap <leader>gs  :action GotoSuperMethod<cr>
nnoremap <leader>ic  :action InspectCode<cr>
nnoremap <leader>io  :action OptimizeImports<cr>
nnoremap <leader>ire :action RenameElement<cr>
nnoremap <leader>irf :action RenameFile<cr>
nnoremap <leader>isu :action ShowUsages<cr>
nnoremap <leader>ifu :action FindUsages<cr>
nnoremap <leader>ib  :action ToggleLineBreakpoint<cr>
nnoremap <leader>ivb :action ViewBreakpoints<cr>
nnoremap [e :action GotoPreviousError<cr>
nnoremap ]e :action GotoNextError<cr>
